{
	"name": "mytraveljournal",
	"version": "0.1.0",
	"private": true,
	"description": "The frontend website for MyTravelJournal",
	"keywords": [
		"next.js",
		"frontend"
	],
	"scripts": {
		"dev": "set NODE_ENV=development && next dev",
		"dev:lambda": "set NODE_ENV=development && netlify-lambda serve src/lambda",
		"build": "next build",
		"build:lambda": "netlify-lambda build src/lambda",
		"start": "set NODE_ENV=production && next start",
		"start:lambda": "set NODE_ENV=production && netlify-lambda serve src/lambda",
		"lint": "next lint",
		"generategraph": "python backend/generate_neo4j_graph_from_md.py",
		"backendstart:dev": "set NODE_ENV=development && cd backend/graphql && node index.js",
		"backendstart:prod": "set NODE_ENV=production && cd backend/graphql && node index.js"
	},
	"dependencies": {
		"@apollo/client": "^3.8.1",
		"@apollo/server": "^4.9.5",
		"@as-integrations/next": "^2.0.2",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.14.12",
		"@mui/material": "^5.14.12",
		"@types/d3-color": "3.1.0",
		"apollo-server-lambda": "^3.12.1",
		"axios": "^1.6.0",
		"graphql": "^16.8.1",
		"interweave": "^13.1.0",
		"lucide-react": "^0.288.0",
		"next": "^13.4.19",
		"react": "^18.2.0",
		"react-dom": "18.2.0",
		"react-simple-maps": "^3.0.0",
		"sharp": "^0.32.6"
	},
	"devDependencies": {
		"@types/node": "18.0.3",
		"@types/react": "18.0.15",
		"@types/react-dom": "18.0.6",
		"@types/react-simple-maps": "^3.0.1",
		"eslint": "8.19.0",
		"eslint-config-next": "12.2.2",
		"netlify-lambda": "^2.0.16",
		"prettier": "^3.0.3",
		"typescript": "4.7.4"
	}
}
